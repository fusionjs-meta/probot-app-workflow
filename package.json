      "probot-app-delete-merged-branch",
      "probot-app-deploy",
      "probot-app-merge-pr",
      "probot-app-monorepo-sync",
      "probot-app-pr-title"
    ]
  },
  "devDependencies": {
    "babel-jest": "^23.2.0",
    "babel-preset-env": "^1.7.0",
